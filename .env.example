# --- Provider API Keys (set only the ones you need) ---
SILICONFLOW_API_KEY=sk-your-key-here
SILICONFLOW_BASE_URL=https://api.siliconflow.cn/v1
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
OPENROUTER_API_KEY=
MINERU_API_KEY=

# --- Default Provider ---
# Used for model names without a provider prefix (e.g. "Qwen/Qwen3-VL-235B-A22B-Instruct")
# Options: siliconflow, openai, anthropic, openrouter, gemini, mistral, groq, deepseek
AE_DEFAULT_PROVIDER=siliconflow

# --- LLM Models ---
# Models can use a provider prefix: openai/gpt-4o, anthropic/claude-sonnet-4-20250514
# Without a prefix, models route through AE_DEFAULT_PROVIDER
AE_WORKER_MODEL=Qwen/Qwen3-VL-235B-A22B-Instruct
AE_BUILDER_MODEL=Pro/zai-org/GLM-5
AE_OBSERVER_MODEL=Pro/moonshotai/Kimi-K2.5
AE_OBSERVER_VISION_MODEL=Qwen/Qwen3-VL-235B-A22B-Instruct

# --- Model Downgrade Tiers (largest to smallest) ---
# Supports mixed providers: openai/gpt-4o,anthropic/claude-haiku-4-5-20251001,Qwen/Qwen3-8B
AE_WORKER_MODEL_TIERS=Qwen/Qwen3-VL-235B-A22B-Instruct,Qwen/Qwen3-32B,Qwen/Qwen3-14B,Qwen/Qwen3-8B

# --- Database ---
AE_DATABASE_URL=sqlite:///./data/anything_extractor.db
# AE_DATABASE_URL=postgresql://localhost:5432/anything_extractor
AE_REDIS_URL=

# --- Language (en | zh | bilingual) ---
AE_LANGUAGE=bilingual

# --- Evolution ---
MAX_ITERATIONS=20

# --- Paths ---
AE_WORKFLOWS_DIR=./workflows
AE_DATA_DIR=./data
